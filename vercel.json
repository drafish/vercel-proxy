{
  "version": 2,
  "public": false,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node",
      "config": {
        "functions": {
          "index.js": {
            "maxDuration": 30
          }
        }
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "index.js"
    }
  ],
  "buildCommand": "npm run build"
}
